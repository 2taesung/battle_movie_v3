          <input v-model=\"userInfo.password\" type=\"password\" class=\"form-control\" id=\"password\">\n        </div>\n        <button @click.prevent=\"login\" type=\"submit\" class=\"btn btn-primary\">로그인</button>\n      </form>\n    </div>\n  </div>\n  \n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      userInfo: {\n        username: '',\n        password: '',\n      \n      },\n    }\n  },\n  methods: {\n    login: function() {\n      axios({\n        method: 'post',\n        url: 'http://localhost:8000/api/v1/accounts/api-token-auth/',\n        data: this.userInfo\n      })\n        .then(res => {\n          console.log(res)\n          localStorage.setItem('jwt', res.data.token)\n          this.$router.push('/home')\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    }\n\n  }\n}\n</script>\n\n<style>\n</style>"]}]}